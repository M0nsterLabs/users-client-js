{
  "name": "tm-user-client-js",
  "version": "1.0.3",
  "description": "User API SDK",
  "main": "index.js",
  "scripts": {
    "predoc": "npm run build",
    "doc": "rimraf doc && jsdoc --configure etc/jsdoc.json dist",
    "build": "rimraf dist && babel -d dist src",
    "clean": "rimraf doc dist",
    "test": "./node_modules/.bin/mocha --compilers js:babel-register --require babel-polyfill --require bluebird --require isomorphic-fetch",
    "test:coverage": "./node_modules/.bin/istanbul cover --report clover ./node_modules/.bin/_mocha -- -u exports -R spec test/**/*  --compilers js:babel-register --require babel-polyfill --require bluebird --require isomorphic-fetch",
    "release": "npm version patch && git push && git push --tags && npm publish",
    "prepublish": "npm run build && npm run doc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M0nsterLabs/users-client-js.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/M0nsterLabs/users-client-js/issues"
  },
  "homepage": "https://github.com/M0nsterLabs/users-client-js#readme",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-latest": "^6.22.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^1.0.0-alpha",
    "jsdoc": "^3.4.3",
    "jsdoc-strip-async-await": "^0.1.0",
    "mocha": "^3.2.0",
    "nock": "^9.0.6",
    "rimraf": "^2.6.0",
    "sinon": "^1.17.7"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  }
}
